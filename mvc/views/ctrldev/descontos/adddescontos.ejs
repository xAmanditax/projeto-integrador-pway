<body>
    <body>
        <main>
            <section id="cadcupt">
                <h2>Cadastrar Descontos</h2>
                <form action="/pagina/caddescontos" method="post">
            <div class="row">
                <div class="col">
                    <label for="txtnomedesco" class="form-label">Imagem</label>
                    <input type="text" name="txtnomedesco" id="txtnomedesco" class="form-control" tabindex="1">
                </div> 
                <div class="col">
                    <label for="txtcoddesco" class="form-label">CÃ³digo</label>
                    <input type="text" name="txtcoddesco" id="txtcoddesco" class="form-control" tabindex="2">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="txtvaldesco" class="form-label">Valor</label>
                    <input type="text" name="txtvaldesco" id="txtvaldesco" class="form-control" tabindex="3">
                </div>
                <div class="col">
                    <label for="txtvalidesco" class="form-label">Validade</label>
                    <input type="datetime-local" name="txtvalidesco" id="txtvalidesco" class="form-control" tabindex="4">
